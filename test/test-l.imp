begin

  function @min(a, b) do
    if a <= b then
      c := a
    else
      c := b
    endif;
    return c
  end;

  function @max(a, b) do
    if a >= b then
      c := a
    else
      c := b
    endif;
    return c
  end;

  a := 5;
  print(a);
  (* this is special because there is no scope *)
  b := @max(a, 8);
  print(b)
end
